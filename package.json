{
  "name": "trip-planner",
  "author": "Your Name",
  "private": true,
  "scripts": {
    "setup": "node setup.mjs",
    "setup:bash": "chmod +x setup.sh && ./setup.sh",
    "validate-setup": "node validate-setup.mjs",
    "dev": "concurrently \"turbo dev --filter=web\" \"langgraphjs dev --no-browser\"",
    "build": "turbo build",
    "turbo:command": "turbo",
    "format": "turbo format",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "test:unit:web": "turbo test:unit --filter=web",
    "test:unit:agents": "turbo test --filter=agents",
    "test:e2e": "turbo test:e2e --filter=web",
    "test:watch": "concurrently \"turbo test:unit --filter=web -- --watch\" \"turbo test --filter=agents -- --watch\""
  },
  "devDependencies": {
    "turbo": "latest",
    "tsx": "^4.19.1",
    "typescript": "^5",
    "eslint": "^9.19.0",
    "concurrently": "^9.1.2",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@eslint/eslintrc": "^3.3.0",
    "@typescript-eslint/parser": "^8.26.1",
    "@tsconfig/recommended": "^1.0.8",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-no-instanceof": "^1.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "prettier": "^3.3.3",
    "@langchain/langgraph-cli": "^0.0.30"
  },
  "resolutions": {
    "@langchain/core": "^0.3.42"
  },
  "packageManager": "pnpm@10.6.3"
}
